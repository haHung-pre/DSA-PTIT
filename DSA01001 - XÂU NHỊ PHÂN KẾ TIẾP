#include<iostream>
using namespace std;
int main(){
	int t;cin>>t;
	while(t--){
		string s;cin>>s;
		int i=s.size()-1;
		while(i>=0&&s[i]=='1'){
			s[i]='0';
			i--;
		}
		if(i==-1){
		for(int j=0;j<=s.size()-1;j++){
			cout<<'0';	
	}
	cout<<endl;
		}else{
			s[i]='1';
			cout<<s<<endl;
		}
	}
}
